{
  "name": "system-viewer",
  "displayName": "System Viewer",
  "description": "Introspect the ROS network.",
  "publisher": "Open Robotics",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/extension.js",
  "scripts": {
    "build": "fox build",
    "prepare": "husky install",
    "foxglove:prepublish": "fox build --mode development",
    "local-install": "fox install",
    "lint": "eslint src --ignore-path .gitignore --max-warnings 0",
    "prettier": "prettier --ignore-path .gitignore \"**/*.(js|jsx|json|yml|yaml|css|ts|tsx|md|mdx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "package": "fox package",
    "pretest": "fox pretest",
    "validate": "npm-run-all check-format lint build"
  },
  "devDependencies": {
    "@foxglove/eslint-plugin": "0.19.0",
    "@foxglove/fox": "0.2.1",
    "@foxglove/studio": "1.7.0",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@types/styled-components": "^5.1.25",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-flow-renderer": "9.6.8",
    "react-svg-loader": "^3.0.3",
    "svg-inline-loader": "^0.8.2",
    "svg-url-loader": "^7.1.1",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@fluentui/react": "^8.66.2",
    "@foxglove/studio-base": "^1.8.0",
    "@mdi/svg": "^6.6.96",
    "@types/parse-json": "^4.0.0",
    "calculate-size": "^1.1.1",
    "classnames": "^2.3.1",
    "elkjs": "^0.7.1",
    "styled-components": "^5.3.5",
    "tinycolor2": "^1.4.2"
  }
}
